{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "htf.marsap",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "htf.marsap.view.Map",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "htf.marsap.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "htf.marsap.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition":"slide",
				"async": true
			},
			"routes": [{
				"name": "map",
				"pattern": "",
				"target": ["map"]
			}, {
				"name": "sensorDetail",
				"pattern": "sensorDetail",
				"target": ["sensorDetail"]
			}],
			"targets": {
				"map": {
					"viewName": "Map",
					"viewLevel": 1
				},
				"sensorDetail": {
					"viewName": "SensorDetail",
					"viewLevel": 1
				}
			}
		}
	},
	"sap.platform.hcp": {
			"uri": "webapp",
			"_version": "1.1.0"
	}
}